<div >
  Meet the Trainers
</div>
<div style="background-image: url('<%= image_path 'wallpaper.png', crop: :fill%> ');">
  <div class="container mt-3">
    <div class="row mb-3">
      <%= render 'search' %>
    </div>
    <div class="d-flex justify-content-center">
      <div class="card-grid">
        <%= render "card_trainer" %>
      </div>
    </div>
  </div>
  <div class="col-4">
    <div class="map-container">
      <div class="map-view"
        data-controller="map"
        data-map-markers-value="<%= @markers.to_json %>"
        data-map-api-key-value="<%= ENV['MAPBOX_API_KEY'] %>">
      </div>
    </div>
  </div>
</div>
